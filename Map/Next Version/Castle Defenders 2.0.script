function InRange(Number, Min, Max: Integer): Boolean;
begin
  Result := ((Number >= Min) and (Number <= Max));
end;


procedure AutoFeed;
var
  UnitsToFeed: array of Integer;
begin
  for I := 0 to 8 do
  begin
    if States.PlayerEnabled(I) then
      UnitsToFeed := States.PlayerGetAllUnits(I);
    for K := 0 to length(UnitsToFeed) - 1 do
      Actions.UnitHungerSet(UnitsToFeed[K], States.UnitMaxHunger);
  end;
end;


function RandomObject: Integer;
begin
  case States.KaMRandomI(100) of
    0..39:  Result := 8;
    40..69: Result := 60;
    70..84: Result := 21;
    85..99: Result := 220;
  end;
end;


function LateBonusName(X, Y: Integer): ANSIString;
begin
  case States.MapTileObject(X, Y) of
    8:   Result := '<$62>';
    60:  Result := '<$63>';
    21:  Result := '<$64>';
    220: Result := '<$65>'
    else Result := '';
  end;
end;


function LateBonusDescription(X, Y: Integer): ANSIString;
begin
