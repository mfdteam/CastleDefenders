procedure CheckBonus;
begin
  case States.KaMRandomI(150) of

    0..19: begin
             bImmediateKill := True;
             if not bMsg then
             begin
               Actions.ShowMsg(-1, '<$6> ' + '<$7>: ' + '<$13>');
               bMsg := True;
             end;
           end;

    20..39: begin
              bDouble := True;
              if not bMsg then
              begin
                Actions.ShowMsg(-1, '<$6> ' + '<$8>: ' + '<$14>');
                bMsg := True;
              end;
            end;

    40..59: begin
              bNewConscription := True;
