procedure OnGroupOrderSplit(G1, G2: Integer);
begin
  if not ENABLE_SPLITTING then
    CheckSplittedGroups(States.GroupOwner(G1), True);
end;


procedure OnUnitDied(aUnitID, aKillerIndex: Integer);
var
  Addition, GroupToLink, Owner, Owner1, GT, UT, F: Integer;
begin
  if (aKillerIndex = -1)
  and (States.UnitHunger(aUnitID) > 0) then
    Exit;

  if (Utils.InRangeI(States.UnitOwner(aUnitID), 4, 7))
  and (Utils.InRangeI(aKillerIndex, 0, 3))
  and (States.UnitHunger(aUnitID) > 400) then
    Scores[aKillerIndex] := Scores[aKillerIndex] + 1;

  if Utils.InRangeI(aKillerIndex, 0, 3) then
    UnlockWarriors(aKillerIndex);

  if Utils.InRangeI(States.UnitOwner(aUnitID), 0, 3) then
    Owner := States.UnitOwner(aUnitID);

  if Utils.InRangeI(States.UnitOwner(aUnitID), 4, 7) then
    Owner1 := aKillerIndex;

  if Utils.InRangeI(States.UnitType(aUnitID), 14, 27) then
  begin
    UT := States.UnitType(aUnitID);

    if bReborn then
      if States.KaMRandom < (REBORN_CHANCE / DifficultyFactor) then
        if (Utils.InRangeI(States.UnitOwner(aUnitID), 0, 3)) then
        begin
          Addition := Actions.GiveGroup(Owner, UT, SpawnX[Owner], SpawnY[Owner], 4, 1, 1);
          GT := States.GroupType(Addition);
          Actions.GroupHungerSet(Addition, States.UnitMaxHunger);
          GroupToLink := FindClosestGroup(Owner, States.UnitPositionX(aUnitID), States.UnitPositionY(aUnitID), GT, AUTOLINK_RADIUS);
          if (GroupToLink > 0)
          and (Addition > 0) then
          begin
            F := States.GroupColumnCount(GroupToLink);
            Actions.GroupOrderLink(Addition, GroupToLink);
            Actions.GroupSetFormation(GroupToLink, Utils.IfThenI(States.GroupManualFormation(GroupToLink), F, 1 + Round(sqrt(States.GroupMemberCount(GroupToLink)))));
          end;
        end;

    if bNewborn then
      if States.KaMRandom < (NEWBORN_CHANCE / DifficultyFactor) then
        if (Utils.InRangeI(aKillerIndex, 0, 3))
        and (Utils.InRangeI(States.UnitOwner(aUnitID), 4, 7)) then
        begin
          Addition := Actions.GiveGroup(Owner1, UT, SpawnX[Owner1], SpawnY[Owner1], 4, 1, 1);
          GT := States.GroupType(Addition);
          Actions.GroupHungerSet(Addition, States.UnitMaxHunger);
          GroupToLink := FindClosestGroup(Owner1, States.UnitPositionX(aUnitID), States.UnitPositionY(aUnitID), GT, AUTOLINK_RADIUS);
          if (GroupToLink > 0)
          and (Addition > 0) then
          begin
            F := States.GroupColumnCount(GroupToLink);
            Actions.GroupOrderLink(Addition, GroupToLink);
            Actions.GroupSetFormation(GroupToLink, Utils.IfThenI(States.GroupManualFormation(GroupToLink), F, 1 + Round(sqrt(States.GroupMemberCount(GroupToLink)))));
          end;
        end;

  end;

end;


procedure OnUnitWounded(aUnitID, aAttacker: Integer);
begin

  if bImmediateKill then
    if States.KaMRandom < (KILL_CHANCE / DifficultyFactor) then
    begin
      if (Utils.InRangeI(States.UnitOwner(aAttacker), 0, 3))
      and (States.UnitHunger(aUnitID) > 400) then
        inc(Scores[States.UnitOwner(aAttacker)]);
      if Utils.InRangeI(States.UnitOwner(aUnitID), 4, 7) then
        Actions.UnitKill(aUnitID, False);
    end;

  if bRetribution then
    if States.KaMRandom < (RETRIBUTION_CHANCE / DifficultyFactor) then
    begin
      if (Utils.InRangeI(States.UnitOwner(aUnitID), 0, 3))
      and (Utils.InRangeI(States.UnitOwner(aAttacker), 4, 7))
      and (States.UnitHunger(aAttacker) > 400) then
      begin
        inc(Scores[States.UnitOwner(aUnitID)]);
        Actions.UnitKill(aAttacker, False);
      end;
    end;

  if bArmor then
    if States.KaMRandom < (ARMOR_CHANCE / DifficultyFactor) then
    begin
      if (Utils.InRangeI(States.UnitOwner(aUnitID), 0, 3))
      and (Utils.InRangeI(States.UnitType(aUnitID), 14, 27))
      and (Utils.InRangeI(States.UnitOwner(aAttacker), 4, 7)) then
        Actions.UnitHPChange(aUnitID, 1);
    end;

end;


procedure OnWarriorEquipped(aUnitID, aGroupID: Integer);
var
  Rogue, Barb, Owner, Addition, F, G1: Integer;
  GetDouble: Boolean;
begin

  if aGroupID <= 0 then
    Exit;

  if aUnitID <= 0 then
    Exit;

  Owner := States.UnitOwner(aUnitID);
  Actions.UnitHungerSet(aUnitID, States.UnitMaxHunger);

  if bDouble then
    if States.KaMRandom < (DOUBLE_CHANCE / DifficultyFactor) then
    begin
      if ((States.UnitType(aUnitID) > 14)
      and not (Utils.InRangeI(States.UnitType(aUnitID), 17, 18)))
      or ((States.UnitType(aUnitID) = 14)
      and (not States.HouseRepair(Barracks[Owner]))) then
      begin 
        Addition := Actions.GiveGroup(Owner, States.UnitType(aUnitID), States.HousePositionX(Barracks[Owner]), States.HousePositionY(Barracks[Owner]) + 1, 4, 1, 1);
        F := States.GroupColumnCount(aGroupID);
        Actions.GroupHungerSet(Addition, States.UnitMaxHunger);
        Actions.GroupOrderLink(Addition, aGroupID);
        Actions.GroupSetFormation(aGroupID, Utils.IfThenI(States.GroupManualFormation(aGroupID), F, 1 + Round(sqrt(States.GroupMemberCount(aGroupID)))));
      end
      else
      if Utils.InRangeI(States.UnitType(aUnitID), 17, 18)
      and (States.StatUnitMultipleTypesCount(Owner, [17, 18]) < Utils.RoundToUp(MAX_RANGED / DifficultyFactor, 5)) then
      begin 
        Addition := Actions.GiveGroup(Owner, States.UnitType(aUnitID), States.HousePositionX(Barracks[Owner]), States.HousePositionY(Barracks[Owner]) + 1, 4, 1, 1);
        F := States.GroupColumnCount(aGroupID);
        Actions.GroupHungerSet(Addition, States.UnitMaxHunger);
        Actions.GroupOrderLink(Addition, aGroupID);
        Actions.GroupSetFormation(aGroupID, Utils.IfThenI(States.GroupManualFormation(aGroupID), F, 1 + Round(sqrt(States.GroupMemberCount(aGroupID)))));
      end;
    end;

  if bRanged then
    if States.KaMRandom < (RANGED_CHANCE / DifficultyFactor) then
      if Utils.InRangeI(States.UnitType(aUnitID), 14, 16)
      and (States.StatUnitMultipleTypesCount(Owner, [17, 18]) < Utils.RoundToUp(MAX_RANGED / DifficultyFactor, 5)) then
      begin 
        Addition := Actions.GiveGroup(Owner, States.KaMRandomI(2) + 17, States.HousePositionX(Barracks[Owner]), States.HousePositionY(Barracks[Owner]) + 1, 4, 1, 1);
        Actions.GroupHungerSet(Addition, States.UnitMaxHunger);
        G1 := FindClosestGroup(Owner, States.UnitPositionX(aUnitID), States.UnitPositionY(aUnitID), 2, AUTOLINK_RADIUS);
        if G1 > 0 then
        begin
          F := States.GroupColumnCount(G1);
          Actions.GroupOrderLink(Addition, G1);
          Actions.GroupSetFormation(G1, Utils.IfThenI(States.GroupManualFormation(G1), F, 1 + Round(sqrt(States.GroupMemberCount(G1)))));
        end;
      end;

  if (not Unlocked[Owner][23])
  and (States.HouseRepair(Barracks[Owner]))
  and (States.HouseResourceAmount(Barracks[Owner], 20) > 3)
  and (States.UnitType(aUnitID) = 14) then
  begin
    GetDouble := (bDouble) and (States.KaMRandom < (DOUBLE_CHANCE / DifficultyFactor));
    Barb := Actions.GiveGroup(Owner, Utils.IfThenI(States.KaMRandom > 0.5, 23, 26), States.UnitPositionX(aUnitID), States.UnitPositionY(aUnitID) + 1, 4, Utils.IfThenI(GetDouble, 2, 1), 1);
    Actions.HouseTakeWaresFrom(Barracks[Owner], 20, 3);
    Actions.GroupHungerSet(Barb, States.UnitMaxHunger);
    F := States.GroupColumnCount(aGroupID);
    Actions.GroupOrderLink(Barb, aGroupID);
    Actions.GroupSetFormation(aGroupID, Utils.IfThenI(States.GroupManualFormation(aGroupID), F, 1 + Round(sqrt(States.GroupMemberCount(aGroupID)))));
    Actions.UnitKill(aUnitID, True);
  end;

  if States.UnitType(aUnitID) = 14 then
    if States.KaMRandom < (ROGUE_CHANCE / DifficultyFactor) then
    begin
      G1 := FindClosestGroup(States.UnitOwner(aUnitID), States.UnitPositionX(aUnitID), States.UnitPositionY(aUnitID), 2, AUTOLINK_RADIUS * 2);
      Rogue := Actions.GiveGroup(States.UnitOwner(aUnitID), 25, States.UnitPositionX(aUnitID), States.UnitPositionY(aUnitID) + 1, 4, 1, 1);
      Actions.GroupHungerSet(Rogue, States.UnitMaxHunger);
      if G1 > 0 then
      begin
        F := States.GroupColumnCount(G1);
        Actions.GroupOrderLink(Rogue, G1);
        Actions.GroupSetFormation(G1, Utils.IfThenI(States.GroupManualFormation(G1), F, 1 + Round(sqrt(States.GroupMemberCount(G1)))));
      end
      else
      if G1 <= 0 then
        if (States.HouseBarracksRallyPointX(Barracks[Owner]) > 0)
        and (States.HouseBarracksRallyPointY(Barracks[Owner]) > 0) then
          Actions.GroupOrderWalk(Rogue, States.HouseBarracksRallyPointX(Barracks[Owner]), States.HouseBarracksRallyPointY(Barracks[Owner]), States.KaMRandomI(8));
    end;
end;


procedure GetReinforce;
var
  R, C, UT: array[0..3] of Integer;
begin
  for I := 0 to 3 do
    if States.PlayerEnabled(I)
    and not (States.PlayerDefeated(I)) then
      begin
        C[I] := 12 + States.KaMRandomI(5);
        if States.KaMRandom < 0.33 then
          UT[I] := 16
        else
        if Utils.InRangeI(Round(100 * States.KaMRandom), 33, 66) then 
          UT[I] := 20
        else
          UT[I] := 22;
        if States.KaMRandom < 0.1 then
          UT[I] := Utils.IfThenI(States.KaMRandom > 0.5, 23, 26);
        R[I] := Actions.GiveGroup(I, UT[I], SpawnX[I], SpawnY[I], 4, C[I], 3 + States.KaMRandomI(3));
        Actions.GroupHungerSet(R[I], States.UnitMaxHunger);
        Actions.ShowMsgGoToFormatted(I, SpawnX[I], SpawnY[I], '<$109>', [C[I], States.UnitTypeName(UT[I])]);
      end;
end;


